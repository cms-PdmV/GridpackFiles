{
    "scheme": "massZ.dat",

    "run_card":{
        "parton_shower": "pythia8",
        "ickkw": "3",
        "maxjetflavor": "5"
    },

    "user":[
        "set run_card mll_sf 50",
        "set run_card ptj 10",
        "set run_card jetradius 1.2"
    ],

    "fragment":[
            "JetMatching:qCut = 30.",
            "JetMatching:qCutME = 10.",
            "JetMatching:nQmatch = 5",
            "JetMatching:nJetMax = 2",
            "TimeShower:mMaxGamma = 4.0"
    ]

}

