import model loop_sm-ckm_no_b_mass

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

generate p p > ell+ vl a [QCD] @0
add process p p > ell+ vl a j [QCD] @1
add process p p > ell- vl~ a [QCD] @2
add process p p > ell- vl~ a j [QCD] @3

output WGtoLNuG-1Jets_PTG-200to400_amcatnloFXFX-pythia8 -nojpeg
