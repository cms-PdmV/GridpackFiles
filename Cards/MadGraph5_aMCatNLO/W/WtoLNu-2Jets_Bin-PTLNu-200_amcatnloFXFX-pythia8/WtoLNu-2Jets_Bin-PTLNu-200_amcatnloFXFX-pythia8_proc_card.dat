import model loop_sm-ckm_no_b_mass

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

generate p p > ell+ vl [QCD] @0
add process p p > ell- vl~ [QCD] @1
add process p p > ell+ vl j [QCD] @2
add process p p > ell- vl~ j [QCD] @3
add process p p > ell+ vl j j [QCD] @4
add process p p > ell- vl~ j j [QCD] @5

output WtoLNu-2Jets_Bin-PTLNu-200_amcatnloFXFX-pythia8 -nojpeg
